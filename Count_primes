class Solution:
    def countPrimes(self, n: int) -> int:

        if n<=2:
            return 0
      
        primes = [1]*(n+1)
        primes[0] = 0
        primes[1] = 0

        for i in range(2,n):
            if primes[i] == 0:
                continue
            for j in range(i*i,n+1,i):
                primes[j] = 0
        c = 0
        for i in range(n):
            if primes[i] == 1:
                c+=1
        return c
                        
